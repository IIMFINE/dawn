#log4cplus.config

#The logger Instance is named DAWN_LOG

log4cplus.logger.DAWN_LOG=ALL, logger

log4cplus.rootLogger =INFO
log4cplus.appender.MyAppend.Append = false

log4cplus.appender.logger=log4cplus::TimeBasedRollingFileAppender
log4cplus.appender.logger.FilenamePattern=./%d{MM-dd}_output_.log
log4cplus.appender.logger.Append=false
log4cplus.appender.logger.MaxHistory=9999
log4cplus.appender.logger.ImmediateFlush=true
log4cplus.appender.logger.RollOnClose=false
log4cplus.appender.logger.CreateDirs=true
log4cplus.appender.logger.layout=log4cplus::PatternLayout
log4cplus.appender.logger.layout.ConversionPattern=[%T] %l %D{%Y-%m-%d %H:%M:%S.%q} %-5p - %m %n